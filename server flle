learn terraform
resource "aws_instance" "frontend" {
  ami           = ami-0b5a2b5b8f2be4ec2
  instance_type = "t3.micro"

  tags = {
    Name = "frontend"
  }
}
output "frontend" {
value = aws_instance.frontend.public_ip
}
